<h1>Listing institutions</h1>

<table>
  <tr>
    <th>Institution</th>
    <th>City</th>
    <th>State</th>
    <th>Attended from</th>
    <th>Attended to</th>
    <th>Degree</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @institutions.each do |institution| %>
  <tr>
    <td><%= institution.institution %></td>
    <td><%= institution.city %></td>
    <td><%= institution.state_id %></td>
    <td><%= institution.attended_from %></td>
    <td><%= institution.attended_to %></td>
    <td><%= institution.degree %></td>
    <td><%= link_to 'Show', institution %></td>
    <td><%= link_to 'Edit', edit_institution_path(institution) %></td>
    <td><%= link_to 'Destroy', institution, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Institution', new_institution_path %>
